<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WeiboBlog.WebLoader"
             Title="WebLoader">
    <VerticalStackLayout x:Name="layout">
        <Label x:Name="label" IsVisible="false"/>
        <WebView x:Name="webview"/>
        <Button x:Name="button" Text="AllLoaded" Clicked="Button_Clicked"/>
        <Button x:Name="printBtn" Text="Extern Browser" Clicked="Button_Clicked_1" IsVisible="false"/>
        <Button x:Name="copy" Text="Copy Source Code" Clicked="Button_Clicked_2" IsVisible="false"/>
        <Button x:Name="save" Text="分享源代码" Clicked="save_Clicked" IsVisible="false"/>
        <Button Text="DebugServer" Clicked="Button_Clicked_3" IsVisible="false"/>
    </VerticalStackLayout>
</ContentPage>
